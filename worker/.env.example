# Database (shared with prompt-service)
DATABASE_URL="file:./dev.db"
# SQLCipher encryption key - 32+ characters recommended
# Must match the DATABASE_KEY used by prompt-service
# Generate with: openssl rand -hex 32
# DATABASE_KEY=

# Environment
NODE_ENV=development

# Logging
LOG_LEVEL=info

# Job Configuration - Token Cleanup
# Enable/disable the token cleanup job (default: true)
JOB_TOKEN_CLEANUP_ENABLED=true

# Cron schedule for token cleanup (default: every hour)
# Format: "minute hour day month day-of-week"
# Examples:
#   "0 * * * *" - every hour at minute 0
#   "*/30 * * * *" - every 30 minutes
#   "0 0 * * *" - daily at midnight
JOB_TOKEN_CLEANUP_SCHEDULE="0 * * * *"
