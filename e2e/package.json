{
  "name": "@votive/e2e",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:ui": "playwright test --ui",
    "e2e:failed": "playwright test --last-failed",
    "e2e:grep": "playwright test --grep",
    "e2e:report": "playwright show-report",
    "e2e:codegen": "playwright codegen",
    "e2e:trace": "playwright test --trace on",
    "e2e:serial": "playwright test --workers=1",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@playwright/test": "^1.49.0",
    "typescript": "^5.9.3"
  }
}
